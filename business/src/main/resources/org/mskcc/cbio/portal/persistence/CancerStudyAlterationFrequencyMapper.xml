<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.mskcc.cbio.portal.persistence.CancerStudyAlterationFrequencyMapper">

<cache/>

<select id="get" resultType="CancerStudyAlterationFrequency">
	select
                CANCER_STUDY_ID as internalStudyId,
                ENTREZ_GENE_ID as entrezGeneId,
                ALTERATION_TYPE as alterationType,
                FREQUENCY as alterationFrequency,
                CASES as numCases
	from alteration_frequency
        where ENTREZ_GENE_ID in
    <foreach item="item" collection="entrezGeneIds" open="(" separator="," close=")">
        #{item}
    </foreach>
        and CANCER_STUDY_ID in
    <foreach item="item" collection="internalStudyIds" open="(" separator="," close=")">
        #{item}
    </foreach>
</select>
</mapper>